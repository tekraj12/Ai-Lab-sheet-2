# Vacuum Cleaner Reflex Agent

def reflex_vacuum_agent(location, status):
    """
    location: 'A' or 'B'
    status: 'Dirty' or 'Clean'
    """
    
    if status == "Dirty":
        return "Suck"
    elif location == "A":
        return "Move Right"
    elif location == "B":
        return "Move Left"


# Testing the agent
print(reflex_vacuum_agent("A", "Dirty"))  # Output: Suck
print(reflex_vacuum_agent("A", "Clean"))  # Output: Move Right
print(reflex_vacuum_agent("B", "Clean"))  # Output: Move Left
